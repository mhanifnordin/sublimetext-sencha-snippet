	title: '${2}',
	${3:data/store}: [{
		text: '${4}',
		value: '${5}'
	}]
}]
]]></content>
    <scope>source.js</scope>
    <description>Multiple scrollable selection</description>
    <tabTrigger>picker</tabTrigger>
